<%= flash[:notice] %>
<h1>to-do List</h1>

<% @tasks.each do |t| %>
<ul>
  <li>名稱 <%= t.title %></li>
  <li>時間 <%= t.start_at %><%= t.end_at %></li>
  <li>優先 <%= t.priority %></li>
  <li>狀態 <%= t.status %></li>
  <li><%= link_to '詳細內容', task_path(t.id) %></li>
</ul>
<% end %>

<%= link_to '新增事項', new_task_path %>